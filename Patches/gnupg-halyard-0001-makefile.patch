 common/Makefile.am | 4 ++--
 g10/Makefile.am    | 2 +-
 tools/Makefile.am  | 2 +-
 3 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/common/Makefile.am b/common/Makefile.am
index 87d6820..796207f 100644
--- a/common/Makefile.am
+++ b/common/Makefile.am
@@ -190,7 +190,7 @@ endif
 
 t_common_cflags = $(KSBA_CFLAGS) $(LIBGCRYPT_CFLAGS) \
                   $(LIBASSUAN_CFLAGS) $(GPG_ERROR_CFLAGS)
-t_common_ldadd = libcommon.a \
+t_common_ldadd = libcommon.a libcommon_a-init.o \
                  $(LIBGCRYPT_LIBS) $(LIBASSUAN_LIBS) $(GPG_ERROR_LIBS) \
 	         $(LIBINTL) $(LIBICONV)
 
diff --git a/tools/Makefile.am b/tools/Makefile.am
index 340901a..87c98d5 100644
--- a/tools/Makefile.am
+++ b/tools/Makefile.am
@@ -26,7 +26,7 @@ EXTRA_DIST = \
 	ccidmon.c ChangeLog-2011 gpg-connect-agent-w32info.rc
 
 
-AM_CPPFLAGS = -I$(top_srcdir)/common
+AM_CPPFLAGS = $(AM_CFLAGS) -I$(top_srcdir)/common
 include $(top_srcdir)/am/cmacros.am
 
 if HAVE_W32_SYSTEM
